<h1 style="text-align: center;">{{title}}</h1>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Polls</h3>
  </div>
  <div class="panel-body">
      {{#each polls}}
          <a href="/poll/{{this._id}}">{{this.title}}</a><br/>
      {{/each}}
  </div>
</div>
